<div id="table-owners" >
    <ul class="collection with-header owners">
        <li class="collection-header"><h5>Propri√©taires & cavaliers</h5></li>

        {% for owner in paginationOwners['owners']  %}
            {% if owner.name is empty %}
                <li class="collection-item">{{ owner.completeName }}
                    <a class="secondary-content" style="color: #00b1bc;" href="tel:+33{{ owner.phone }}">{{ owner.phone|phone }}</a>
                </li>
            {% else %}
                <li class="collection-item">{{ owner.completeName }} ({{ owner.name }})
                    <a class="secondary-content" style="color: #00b1bc;" href="tel:+33{{ owner.phone }}">{{ owner.phone|phone }}</a>
                </li>
            {% endif %}
        {% endfor %}
    </ul>

    {{ include('dashboard/admin/ajax/pagination/owners.html.twig') }}

</div>

